    "multiCommand.commands": {
        "multiCommand.scrollAndAlign": {
            "command": "multiCommand.scrollAndAlign",
            "interval": 10,
            "sequence": [
                {
                    "command": "vscode-neovim.send",
                    "args": "<C-d>"
                },
                {
                    "command": "vscode-neovim.send",
                    "args": "zz"
                }
            ]
        },
        "multiCommand.scrollUpAndAlign": {
            "command": "multiCommand.scrollAndAlign",
            "interval": 10,
            "sequence": [
                {
                    "command": "vscode-neovim.send",
                    "args": "<C-u>"
                },
                {
                    "command": "vscode-neovim.send",
                    "args": "zz"
                }
            ]
        },
        "multiCommand.moveDownAndAlign": {
            "command": "multiCommand.moveDownAndAlign",
            "interval": 10,
            "sequence": [
                {
                    "command": "vscode-neovim.send",
                    "args": "j"
                },
                {
                    "command": "vscode-neovim.send",
                    "args": "zz"
                }
            ]
        },
        "multiCommand.moveUpAndAlign": {
            "command": "multiCommand.moveUpAndAlign",
            "interval": 10,
            "sequence": [
                {
                    "command": "vscode-neovim.send",
                    "args": "k"
                },
                {
                    "command": "vscode-neovim.send",
                    "args": "zz"
                }
            ]
        },
        "multiCommand.moveNextAndAlign": {
            "command": "multiCommand.moveNextAndAlign",
            "interval": 5,
            "sequence": [
                {
                    "command": "vscode-neovim.send",
                    "args": "n"
                },
                {
                    "command": "vscode-neovim.send",
                    "args": "zz"
                }
            ]
        },
        "multiCommand.movePrevAndAlign": {
            "command": "multiCommand.movePrevAndAlign",
            "interval": 5,
            "sequence": [
                {
                    "command": "vscode-neovim.send",
                    "args": "N"
                },
                {
                    "command": "vscode-neovim.send",
                    "args": "zz"
                }
            ]
        },
    },
