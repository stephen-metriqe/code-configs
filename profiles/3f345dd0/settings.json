{
    "python.analysis.typeCheckingMode": "standard",
    "python.analysis.autoImportCompletions": true,
    "python.analysis.fixAll": [
        "source.unusedImports"
    ],
    "editor.defaultFormatter": "ms-python.black-formatter",
    "files.exclude": {
        "**/__pycache__": true,
        "**/.cache": true,
        "**/.coverage": true,
        "**/.coverage.*": true,
        "**/.hypothesis": true,
        "**/.mypy_cache": true,
        "**/.nox": true,
        "**/.pytest_cache": true,
        "**/.ruff_cache": true,
        "**/.tox": true
    },
    "workbench.colorCustomizations": {
        "editor.selectionHighlightBorder": "#ff00c3",
        "editor.selectionBackground": "#f707ff7b",
        "tree.indentGuidesStroke": "#ff0000",
        "terminal.background": "#131212",
        "terminal.foreground": "#dddad6",
        "terminal.ansiBlack": "#1D2021",
        "terminal.ansiBrightBlack": "#665C54",
        "terminal.ansiBrightBlue": "#0D6678",
        "terminal.ansiBrightCyan": "#8BA59B",
        "terminal.ansiBrightGreen": "#237e02",
        "terminal.ansiBrightMagenta": "#8F4673",
        "terminal.ansiBrightRed": "#FB543F",
        "terminal.ansiBrightWhite": "#FDF4C1",
        "terminal.ansiBrightYellow": "#FAC03B",
        "terminal.ansiBlue": "#00a1f9",
        "terminal.ansiCyan": "#8BA59B",
        "terminal.ansiGreen": "#95C085",
        "terminal.ansiMagenta": "#8F4673",
        "terminal.ansiRed": "#FB543F",
        "terminal.ansiWhite": "#A89984",
        "terminal.ansiYellow": "#FAC03B",
        "jumpy2.labelFontColor": "#000000",
        "jumpy2.labelBackgroundColor": "#d3d3d3",
        "jumpy2.labelBorderColor": "#d3d3d3",
        "jumpy2.beaconColor": "#ff0000af",
        "jumpy2.checkered_labelBackgroundColor": "#d3d3d3",
        "jumpy2.checkered_labelFontColor": "#000000",
        "activityBarBadge.background": "#155e02",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#fa1b49",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#fa1b49",
        "statusBarItem.remoteBackground": "#dd0531",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#dd053199",
        "titleBar.inactiveForeground": "#e7e7e799",
        "editor.lineHighlightBackground": "#cfc2102d",
        "editor.lineHighlightBorder": "#edfd0636",
        "editorCursor.foreground": "#ff4e22"
    },
    "workbench.sideBar.location": "right",
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "workbench.colorTheme": "OrangeFlavor Theme",
    "editor.formatOnSave": true,
    "files.autoSaveWorkspaceFilesOnly": true,
    "files.autoSaveWhenNoErrors": true,
    "workbench.editor.showTabs": "single",
    "editor.fontFamily": "\"Kode Mono Regular\",\"Comic Mono\",Consolas, 'Courier New', monospace",
    "editor.scrollbar.ignoreHorizontalScrollbarInContentHeight": true,
    "editor.scrollbar.vertical": "hidden",
    "interactiveWindow.promptToSaveOnClose": true,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[python]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "ms-python.black-formatter",
    },
    "isort.args": [
        "--profile",
        "black"
    ],
    "python.languageServer": "Pylance",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
        "--load-plugins",
        "pylint_django"
    ],
    "vim.easymotion": true,
    "vim.foldfix": true,
    "vim.replaceWithRegister": true,
    "vim.smartRelativeLine": true,
    "vim.sneak": true,
    "vim.sneakReplacesF": true,
    "vim.sneakUseIgnorecaseAndSmartcase": true,
    "vim.useSystemClipboard": true,
    "vim.leader": "<space>",
    "vim.gdefault": true,
    "vim.highlightedyank.color": "rgba(250, 0, 0, 1)",
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.textColor": "rgba(250, 0, 0, 1)",
    "vim.showMarksInGutter": true,
    "vim.visualstar": true,
    "vim.handleKeys": {
        "<C-d>": false,
        "<C-s>": false,
        "<C-z>": false
    },
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "e"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "e"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "o"
            ],
            "commands": [
                {
                    "command": "editor.action.insertLineAfter",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>",
                "o"
            ],
            "commands": [
                {
                    "command": "editor.action.insertLineBefore",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "after": [
                "v",
                "a",
                "w",
                "p"
            ]
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "after": [
                "*",
                "N"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "*",
                "N",
                "c",
                "g",
                "n"
            ]
        },
        {
            "before": [
                "<leader>",
                "a"
            ],
            "after": [
                "g",
                "g",
                "V",
                "G"
            ]
        },
        {
            "before": [
                "<leader>",
                "c"
            ],
            "after": [
                "y",
                "i",
                "w",
                "o",
                "p",
                "r",
                "e",
                "t",
                "(",
                "<Esc>",
                "p",
                "%"
            ]
        },
    ],
    "files.autoSave": "onFocusChange",
    "workbench.iconTheme": "material-icon-theme",
    "notebook.defaultFormatter": "ms-python.black-formatter",
    "jupyter.interactiveWindow.notebookRepl": true,
    "database-client.autoSync": true,
    "errorLens.gutterIconsEnabled": true,
    "editor.wordWrap": "on",
    "editor.mouseWheelZoom": true,
    "workbench.activityBar.location": "hidden",
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "editor.minimap.renderCharacters": false,
    "editor.minimap.enabled": false,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "todo-tree.general.showActivityBarBadge": true,
    "todo-tree.general.tags": [
        "note",
        "todo",
        "fixme",
        "mark",
        "pending",
        "done"
    ],
    "todo-tree.highlights.defaultHighlight": {
        "type": "tag",
        "fontWeight": "bold",
        "foreground": "#000000",
        "opacity": 90
    },
    "todo-tree.highlights.customHighlight": {
        "note": {
            "background": "#FFA500",
            "iconColour": "#FFA500",
            "icon": "file"
        },
        "todo": {
            "background": "#FFD703",
            "iconColour": "#FFD703",
            "icon": "tasklist"
        },
        "fixme": {
            "background": "#FF01FF",
            "iconColour": "#FF01FF",
            "icon": "bug"
        },
        "mark": {
            "background": "#157EFB",
            "iconColour": "#157EFB",
            "icon": "tag"
        },
        "pending": {
            "background": "#FF2D00",
            "iconColour": "#FF2D00",
            "icon": "square"
        },
        "done": {
            "background": "#00ff0080",
            "iconColour": "#00ff0080",
            "icon": "square-fill"
        }
    },
