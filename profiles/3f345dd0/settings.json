    "database-client.autoSync": true,
    "debug.inlineValues": "on",
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.fontFamily": "'Cascadia Code', Consolas, 'Comic Mono', 'Courier New', monospace, 'Kode Mono Regular'",
    "editor.fontLigatures": "'calt', 'liga', 'clig', 'dlig', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
    "editor.fontSize": 15,
    "editor.fontWeight": "700",
    "editor.formatOnSave": true,
    "editor.lineNumbers": "relative",
    "scm.diffDecorations": "none",
    "errorLens.gutterIconsEnabled": false,
    "debug.gutterMiddleClickAction": "none",
    "editor.showFoldingControls": "never",
    "editor.hideCursorInOverviewRuler": true,
    "editor.overviewRulerBorder": false,
    "editor.glyphMargin": false,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.mouseWheelZoom": true,
    "editor.scrollbar.ignoreHorizontalScrollbarInContentHeight": true,
    "editor.scrollbar.vertical": "hidden",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "constant",
                    "emphasis",
                    "entity",
                    "invalid",
                    "keyword",
                    "markup",
                    "meta",
                    "storage",
                    "string",
                    "strong",
                    "support",
                    "variable"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.wordWrap": "on",
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning",
        "info",
        "hint"
    ],
    "editor.suggestOnTriggerCharacters": true,
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "off",
        "strings": "off"
    },
    "python.analysis.aiCodeActions": {
        "implementAbstractClasses": true,
        "generateSymbol": true,
        "convertFormatString": true
    },
    "python.analysis.languageServerMode": "full",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "editor.snippetSuggestions": "bottom",
    "files.autoSave": "onFocusChange",
    "files.autoSaveWhenNoErrors": true,
    "files.autoSaveWorkspaceFilesOnly": true,
    "files.exclude": {
        "**/__pycache__": true,
        "**/.cache": true,
        "**/.coverage": true,
        "**/.coverage.*": true,
        "**/.hypothesis": true,
        "**/.mypy_cache": true,
        "**/.nox": true,
        "**/.pytest_cache": true,
        "**/.ruff_cache": true,
        "**/.tox": true
    },
    "github.copilot.enable": {
